citHeader("To cite geomultistar in publications use:")

citEntry(entry = "Manual",
  title        = "{geomultistar}: Multidimensional Queries Enriched with Geographic Data",
  author       = personList(as.person("Jose Samos")),
  year         = "2020",
  note         = "R package version 1.1.0",
  url          = "https://CRAN.R-project.org/package=geomultistar",

  textVersion  =
  paste("Jose Samos (2020).",
        "geomultistar: Multidimensional Queries Enriched with Geographic Data.",
        "https://CRAN.R-project.org/package=geomultistar.")
)
