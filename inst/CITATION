citHeader("To cite geomultistar in publications use:")

citEntry(entry = "Manual",
  title        = "{geomultistar}: Geographical Queries on Multidimensional Data",
  author       = personList(as.person("Jose Samos")),
  year         = "2020",
  note         = "R package version 1.0.0",
  url          = "https://CRAN.R-project.org/package=starschemar",

  textVersion  =
  paste("Jose Samos (2020).",
        "geomultistar: Geographical Queries on Multidimensional Data.",
        "https://CRAN.R-project.org/package=starschemar.")
)
